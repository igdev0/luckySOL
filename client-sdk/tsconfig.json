{
  "compilerOptions": {
    "moduleResolution": "NodeNext",
    "target": "ES2023",
    "module": "NodeNext",
    "declaration": true,
    "noEmit": true,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "sourceMap": false,
    "emitDecoratorMetadata": true,
    "allowJs": true,
    "experimentalDecorators": true,
    "forceConsistentCasingInFileNames": true
  },
}